{
  "sound": false,
  "notification": false,
  "timeout": 2,
  "showProjectName": true,
  "notificationSystem": "osascript",
  "command": {
      "enabled": true,
      "path": "/usr/bin/bash",
      "args": ["-c", "test {message} != subagent_complete && dunstify -u low -a opencode $PWD {message}"],
      "minDuration": 1
  },
  "messages": {
    "permission": "Action required",
    "complete": "Done!",
    "subagent_complete": "Subagent complete",
    "error": "Something went wrong",
    "question": "Input needed"
  }
}
